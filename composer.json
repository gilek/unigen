{
    "name": "unigen/unigen",
    "type": "library",
    "description": "UniGen is a unit test generator for PHP. It automatically generates unit tests for your classes ",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Kujawa",
            "email": "thekamilkujawa@gmail.com"
        }
    ],
    "bin": ["bin/test-generator"],
    "keywords": [
        "unit",
        "unit testing",
        "testing"
    ],
    "require": {
        "php": ">=7.0",
        "twig/twig": "^1.0|^2.0",
        "symfony/console": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "UniGen\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5|^6.5|^7.0",
        "mockery/mockery": "^1.0|^1.2",
        "mikey179/vfsstream": "^1.0"
    }
}
