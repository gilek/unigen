{
    "name": "unitgen/unitgen",
    "type": "library",
    "description": "UniGen is a unit test generator for PHP. It automatically generates unit tests for your classes ",
    "authors": [
        {
            "name": "Kamil Kujawa",
            "email": "thekamilkujawa@gmail.com"
        }
    ],
    "keywords": [
        "unit",
        "unit testing",
        "testing"
    ],
    "require": {
        "php": ">=7.0",
        "twig/twig": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "UnitGen\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "mikey179/vfsStream": "^1.6"
    }
}
