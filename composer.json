    "require-dev": {
        "ext-json": "*",
        "justinrainbow/json-schema": "^5.2",
        "symfony/console": "^5.0",
        "twig/twig": "^3.0",
        "mikey179/vfsstream": "^1.0",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3"
    }
}
