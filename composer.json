{
    "name": "unigen/unigen",
    "type": "library",
    "description": "UniGen is a unit test generator for PHP. It automatically generates unit tests for your classes.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Kujawa",
            "email": "thekamilkujawa@gmail.com"
        },
        {
            "name": "Maciej KÅ‚ak",
            "email": "klak.maciej@gmail.com"
        }
    ],
    "bin": ["bin/unigen"],
    "keywords": [
        "mockery",
        "phpunit",
        "unit testing"
    ],
    "require": {
        "php": ">=7.1",
        "phpstan/phpstan": "^0.12.4"
    },
    "autoload-dev": {
        "psr-4": {
            "UniGen\\": "src"
        }
